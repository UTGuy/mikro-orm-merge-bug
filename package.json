{
  "name": "string-array-bug",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "migration:init": "rm -rf ./src/migrations && rm -rf ./temp && npx mikro-orm migration:create --initial",
    "migration:create": "npx mikro-orm migration:create",
    "seed": "npx mikro-orm migration:fresh --seed=DatabaseSeeder",
    "start": "ts-node ./src/index.ts",
    "start:seed": "yarn seed && yarn start",
    "test": "jest"
  },
  "dependencies": {
    "@mikro-orm/core": "^5.6.10-dev.6",
    "@mikro-orm/migrations": "^5.6.10-dev.6",
    "@mikro-orm/postgresql": "^5.6.10-dev.6",
    "@mikro-orm/reflection": "^5.6.10-dev.6",
    "@mikro-orm/sql-highlighter": "^1.0.1",
    "@types/node": "^18.11.18",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@mikro-orm/cli": "^5.6.10-dev.6",
    "@mikro-orm/seeder": "^5.6.10-dev.6",
    "@types/jest": "^29.4.0",
    "jest": "^29.4.2",
    "ts-jest": "^29.0.5"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "src/mikro-orm.config.ts",
      "dist/src/mikro-orm.config.js"
    ],
    "migrations": {
      "path": "dist/migrations",
      "pathTs": "src/migrations"
    }
  }
}
